</head>

<body class="index-page bg-gray-200">
  <style>
    .blog-card-fixed {
      width: 100%;
      max-width: 300px;
      height: 300px;
      overflow: hidden;
      display: flex;
      flex-direction: column;
    }
    .blog-card-fixed img {
      width: 100%;
      height: 170px;
      object-fit: cover;
      display: block;
    }
    .blog-card-fixed h4 {
      margin: 0.5rem 0.75rem 0.25rem;
      font-size: 1rem;
      line-height: 1.25;
      display: -webkit-box;
      -webkit-box-orient: vertical;
      -webkit-line-clamp: 2;
      overflow: hidden;
    }
    .blog-card-fixed h6 {
      margin: 0 0.75rem 0.5rem;
      font-size: 0.85rem;
      line-height: 1.3;
      display: -webkit-box;
      -webkit-box-orient: vertical;
      -webkit-line-clamp: 3;
      overflow: hidden;
    }
    .blog-card-fixed .text-xs {
      margin: 0 0.75rem 0.25rem;
    }
  </style>
  {{> __newnav}}



  <div 
    class=" page-header ima min-vh-180 hero-wrap hero-wrap-2 ftco-degree-bg js-fullheight"

    style="background-image: url('https://images.unsplash.com/photo-1630752708689-02c8636b9141?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2490&q=80')">
    <span class="mask btn bg-gradient-dark opacity-6"></span>
    <div class="container">
      <div class="row">
        <div class="col-md-8 mx-auto">
          <div class="text-center">
            <h1 class="text-white">Read Nick House Buyer Blogs to Ready for Fast Sell House</h1>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="card card-body shadow-xl mx-3 mx-md-4 mt-n6">
    <div class="container">
      <div class="section">
        <div class="mb-5">
          <h2 class="text-center">Release Blog</h2>
          <div class="row mt-4">
            {{#each releaseBlogs}}
            <div class="col-md-4 mb-4 d-flex justify-content-center">
              <a href="/blog/{{this.postId}}" class="center">
                <div class="card move-on-hover blog-card-fixed">
                  <img class="w-100" src="{{photoUrl this.Imag}}" alt="{{#if this.FeaturedImageAltText}}{{this.FeaturedImageAltText}}{{else}}{{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}{{/if}}">
                  <h4 class="">
                    {{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}
                  </h4>
                  <h6 class="text-secondary font-weight-normal">
                    {{#if this.ArticleDescription}}{{this.ArticleDescription}}{{else}}{{this.Description}}{{/if}}
                  </h6>
                </div>
              </a>
            </div>
            {{/each}}
          </div>
        </div>

        <div class="mb-5">
          <h2 class="text-center">Recommend Section</h2>
          <div class="row mt-4">
            {{#each recommendBlogs}}
            <div class="col-md-4 mb-4 d-flex justify-content-center">
              <a href="/blog/{{this.postId}}" class="center">
                <div class="card move-on-hover blog-card-fixed">
                  <img class="w-100" src="{{photoUrl this.Imag}}" alt="{{#if this.FeaturedImageAltText}}{{this.FeaturedImageAltText}}{{else}}{{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}{{/if}}">
                  <h4 class="">
                    {{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}
                  </h4>
                  <h6 class="text-secondary font-weight-normal">
                    {{#if this.ArticleDescription}}{{this.ArticleDescription}}{{else}}{{this.Description}}{{/if}}
                  </h6>
                </div>
              </a>
            </div>
            {{/each}}
          </div>
        </div>

        <div class="mb-5">
          <h2 class="text-center">By Category</h2>
          {{#each categoryGroups}}
          <div class="mt-4">
            <h4 class="mb-3">{{this.category}}</h4>
            <div class="row">
              {{#each this.posts}}
              <div class="col-md-4 mb-4 d-flex justify-content-center">
                <a href="/blog/{{this.postId}}" class="center">
                  <div class="card move-on-hover blog-card-fixed">
                    <img class="w-100" src="{{photoUrl this.Imag}}" alt="{{#if this.FeaturedImageAltText}}{{this.FeaturedImageAltText}}{{else}}{{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}{{/if}}">
                    <h4 class="">
                      {{#if this.ArticleTitle}}{{this.ArticleTitle}}{{else}}{{this.Title}}{{/if}}
                    </h4>
                    <h6 class="text-secondary font-weight-normal">
                      {{#if this.ArticleDescription}}{{this.ArticleDescription}}{{else}}{{this.Description}}{{/if}}
                    </h6>
                  </div>
                </a>
              </div>
              {{/each}}
            </div>
          </div>
          {{/each}}
        </div>

        {{> __newfooter}}
