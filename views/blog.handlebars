<!DOCTYPE html>
<html lang="en" itemscope itemtype="http://schema.org/WebPage">

<head>


  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="{{#if postt.ArticleDescription}}{{sanitize postt.ArticleDescription}}{{else}}{{sanitize postt.Description}}{{/if}}">

    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png">
  <link rel="icon" type="image/png" href="../assets/img/favicon.png">

  <title>{{title}}</title>

  {{#if jsonLd}}
  <script type="application/ld+json">
{{{jsonSafe jsonLd}}}
  </script>
  {{/if}}

  {{#if postt.SeoJsonLd}}
  <script type="application/ld+json">
{{{postt.SeoJsonLd}}}
  </script>
  {{/if}}

  <!--     Fonts and icons     -->
  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700,900" />
  <!-- Nucleo Icons -->
  <link href="/public/css/nucleo-icons.css" rel="stylesheet" />
  <link href="/public/css/nucleo-svg.css" rel="stylesheet" />
  <!-- Font Awesome Icons -->
  <script src="https://kit.fontawesome.com/c417bf6d22.js" crossorigin="anonymous"></script>
  <!-- Material Icons -->
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@24,400,0,0" />
  <!-- CSS Files -->
  <link rel="stylesheet" href="/public/css/mdb.min.css" />
  <link id="pagestyle" href="/public/css/material-kit.css?v=3.1.0" rel="stylesheet" />

</head>

<body>
  {{> __newnav}}

  <div     class=" page-header ima min-vh-180 hero-wrap hero-wrap-2 ftco-degree-bg js-fullheight"
   style="background-image: url('{{photoUrl postt.Imag}}')" alt="{{#if postt.ArticleTitle}}{{postt.ArticleTitle}}{{else}}{{postt.Title}}{{/if}}">
    <span class="mask bg-gradient-dark opacity-6"></span>
    <div class="container">
      <div class="row">
        <div class="col-md-8 mx-auto">
          <div class="text-center">
            <h1 class="text-white">{{#if postt.ArticleTitle}}{{postt.ArticleTitle}}{{else}}{{postt.Title}}{{/if}}</h1>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="card card-body shadow-xl mx-3 mx-md-4 mt-n6">
    <div class="container">
      <div class="section">

        <!-- Courses Course Details Section -->
        <section id="courses-course-details" class="courses-course-details section">

          <div class="container" data-aos="fade-up">

            <div class="row">
              <div class="col-lg-12">
                <p class="py-3 my-3 text-center text-dark">{{#if postt.ArticleDescription}}{{postt.ArticleDescription}}{{else}}{{postt.Description}}{{/if}}</p>
              </div>

              <div class="col-lg-8">
                <div class="mb-3 text-sm text-secondary">
                  {{#if postt.Category}}
                    <div>Category: {{postt.Category}}</div>
                  {{/if}}
                  {{#if postt.Tags}}
                    <div>Tags: {{postt.Tags}}</div>
                  {{/if}}
                  {{#if postt.PrimaryKeyword}}
                    <div>Primary Keyword: {{postt.PrimaryKeyword}}</div>
                  {{/if}}
                  {{#if postt.SecondaryKeywords}}
                    <div>Secondary Keywords: {{postt.SecondaryKeywords}}</div>
                  {{/if}}
                  {{#if postt.Slug}}
                    <div>Slug: {{postt.Slug}}</div>
                  {{/if}}
                  {{#if postt.FeaturedImageIdea}}
                    <div>Featured Image Idea: {{postt.FeaturedImageIdea}}</div>
                  {{/if}}
                  {{#if postt.FeaturedImageAltText}}
                    <div>Featured Image Alt Text: {{postt.FeaturedImageAltText}}</div>
                  {{/if}}
                </div>
                <span>
                  {{#if postt.Contents}}
                    {{{postt.Contents}}}
                  {{else}}
                    {{postt.Content}}
                  {{/if}}
                </span>
                {{#if postt.Cta}}
                <div class="mt-4 p-3 border rounded">
                  <strong>CTA:</strong> {{postt.Cta}}
                </div>
                {{/if}}
              </div>


              <div class="col-md-4">
                <div class="card move-on-hover border shadow-0 mb-3 glass-card py-4 my-4 position-sticky"
                  style="top:100px !important; ">
                  <div class="card-body rounded-4 ">
                    <img class="page-header imac" src="{{photoUrl postt.Imag}}">
                    <P class="text-center  text-dark">{{#if postt.ArticleTitle}}{{postt.ArticleTitle}}{{else}}{{postt.Title}}{{/if}}</P>
                  </div>
                </div>
              </div>

            </div>

          </div>
        </section><!-- /Courses Course Details Section -->




        {{> __newfooter}}


      </div>
    </div>
  </div>

  <script src="/public/js/material-kit.min.js?v=3.1.0" type="text/javascript"></script>
